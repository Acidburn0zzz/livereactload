{
  "name": "livereactload",
  "version": "2.0.0-alpha1",
  "description": "Live code editing with Browserify and React",
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:milankinen/livereactload.git"
  },
  "keywords": [
    "react",
    "livereload",
    "real-time",
    "browserify-plugin",
    "development"
  ],
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run dist",
    "test": "npm run mocha -- test/*.js",
    "dist": "babel src --source-maps inline --modules umd --out-dir lib",
    "mocha": "mocha --timeout 120000 --compilers js:babel/register"
  },
  "dependencies": {
    "cli-color": "1.0.0",
    "node-notifier": "4.2.3",
    "through2": "2.0.0",
    "ws": "0.8.0"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "debug": "2.2.0",
    "extend": "3.0.0",
    "fs-extra": "0.24.0",
    "mocha": "2.3.2",
    "shelljs": "0.5.3",
    "webdriverio": "3.2.3"
  }
}
