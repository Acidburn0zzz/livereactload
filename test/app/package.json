{
  "name": "livereactload-test-app",
  "version": "0.0.1",
  "scripts": {
    "start": "babel-node server.js & npm run watch & wait",
    "bundle": "browserify site.js -t babelify -p [ livereactload --no-server ] > bundle.js",
    "watch": "watchify site.js -v -t babelify -p livereactload -o bundle.js"
  },
  "dependencies": {
    "babel": "5.8.23",
    "babelify": "6.3.0",
    "browserify": "11.1.0",
    "express": "4.13.3",
    "react": "0.13.3"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "1.0.4",
    "livereactload": "file:../..",
    "react-proxy": "1.0.0",
    "watchify": "3.4.0"
  }
}
